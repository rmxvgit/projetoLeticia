module ProjetoP_vlg_vec_tst();
// constants                                           
// general purpose registers
reg CLOCK_50;
reg reset;
reg start;
reg [6:0] SW;
// wires                                               
wire [6:0] HEX0;
wire [6:0] HEX1;
wire [6:0] HEX2;
wire [6:0] HEX3;
wire [6:0] HEX4;
wire [6:0] key;

wire sampler;                             

// assign statements (if any)                          
ProjetoP i1 (
// port map - connection between master ports and signals/registers   
	.CLOCK_50(CLOCK_50),
	.HEX0(HEX0),
	.HEX1(HEX1),
	.HEX2(HEX2),
	.HEX3(HEX3),
	.HEX4(HEX4),
	.key(key),
	.reset(reset),
	.start(start),
	.SW(SW)
);

// CLOCK_50
always
begin
	CLOCK_50 = 1'b0;
	CLOCK_50 = #10000 1'b1;
	#10000;
end 

// reset
initial
begin
	reset = 1'b0;
	reset = #40000 1'b1;
	reset = #40000 1'b0;
	reset = #540000 1'b1;
	reset = #50000 1'b0;
end 

// start
initial
begin
	start = 1'b0;
	start = #120000 1'b1;
	start = #50000 1'b0;
	start = #540000 1'b1;
	start = #60000 1'b0;
end 

// SW[6]
initial
begin
	SW[6] = 1'b1;
	SW[6] = #560000 1'b0;
end 

// SW[5]
initial
begin
	SW[5] = 1'b1;
	SW[5] = #560000 1'b0;
end 

// SW[4]
initial
begin
	SW[4] = 1'b1;
	SW[4] = #560000 1'b0;
end 

// SW[3]
initial
begin
	SW[3] = 1'b1;
	SW[3] = #560000 1'b0;
end 

// SW[2]
initial
begin
	SW[2] = 1'b1;
	SW[2] = #560000 1'b0;
end 

// SW[1]
initial
begin
	SW[1] = 1'b1;
	SW[1] = #560000 1'b0;
end 

// SW[0]
initial
begin
	SW[0] = 1'b1;
	SW[0] = #560000 1'b0;
end 

ProjetoP_vlg_sample_tst tb_sample (
	.CLOCK_50(CLOCK_50),
	.reset(reset),
	.start(start),
	.SW(SW),
	.sampler_tx(sampler)
);

