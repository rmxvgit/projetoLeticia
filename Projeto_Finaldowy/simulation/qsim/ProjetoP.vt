module ProjetoP_vlg_sample_tst(
	CLOCK_50,
	reset,
	start,
	SW,
	sampler_tx
);
input  CLOCK_50;
input  reset;
input  start;
input [6:0] SW;
output sampler_tx;

reg sample;
time current_time;
always @(CLOCK_50 or reset or start or SW)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
